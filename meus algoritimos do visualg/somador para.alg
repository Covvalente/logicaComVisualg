algoritmo "somador para "
// Função :
// Autor :
// Data : 09/05/2023
// Seção de Declarações
var
n,s:inteiro
resp:caractere
inicio
// Seção de Comandos
s<- 0
repita
   escreval ("digite um valor : " )
   leia (n)
   s<-s+n
   escreva (" quer continuar ?(s/n)")
   leia (resp)
Ate (resp="n")
escreval (" a soma dos valores e ",s)

fimalgoritmo
