algoritmo "soma parametro referencia"
// Função :
// Autor :
// Data : 10/05/2023
// Seção de Declarações
var
x,y:inteiro
procedimento soma(var a,b:inteiro)
inicio
a<-a+1
b<-b+2
escreval("valor de A=",a)
escreval("valor de B=",b)
escreval("a soma A+B=",a+b)
fimProcedimento
inicio
x<-4
y<-8
soma(x,y)
escreval(" valor de x =",x)
escreval (" Valor de y=",y)
fimalgoritmo
